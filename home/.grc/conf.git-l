# special characters that titles start with
regexp=^[\w-]+ ([^ A-Za-z0-9.])+\s
colours=white
=======
# colon following "WIP"
regexp=WIP:
colours=cyan
=======
# "WIP" prefix
regexp=^[\w-]+ WIP
colours=white
=======
# commits to delete
regexp=^[\w-]+ --.*
colours=red
=======
# SHAs/dates
regexp=^[\w-]+
colours=yellow
=======
# interpunction following ticket numbers
regexp=(#\d+)|(#?[A-Z][A-Z0-9]*-\d+)[,:]
colours=cyan
=======
# ticket numbers
regexp=(#\d+)|(#?[A-Z][A-Z0-9]*-\d+)
colours=blue
