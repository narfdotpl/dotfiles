[user]
    name = Maciej Konieczny
    email = hello@narf.pl
[core]
    editor = mvim --nofork
    excludesfile = /Users/narf/.gitglobalignore
[alias]
    a = add . --verbose
    b = branch
    c = commit --all --verbose
    ca = commit --all --verbose --amend
    co = checkout
    d = !git diff | mvim --nofork -
    i = !git init && git add . && git commit --message='initial commit'
    l = !git log origin..HEAD --graph --pretty=format:'%cd %s' --date=short \
        | less
    la = !git log --graph --pretty=format:'%cd %s' --date=short | less
    lo = !git log origin --graph --pretty=format:'%cd %s' --date=short | less
    r = rebase --interactive HEAD~10
    rc = rebase --continue
    s = status
    web = instaweb
    who = shortlog --summary --numbered --email
[instaweb]
    httpd = webrick  # OS X love
